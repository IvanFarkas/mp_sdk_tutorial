  "scripts": {
    "start": "webpack serve --config ./webpack.config.js",
    "build-tools": "tsc --outDir ./tools/dist ./tools/echo.ts",
    "fetch-bundle": "cross-var curl https://static.matterport.com/showcase-sdk/bundle/$npm_package_config_bundle_version/showcase-bundle.zip -o bundle.zip",
    "expand-bundle": "yarn decompress bundle.zip --out-dir=./bundle",
    "install-bundle": "yarn fetch-bundle && yarn expand-bundle && rm bundle.zip",
    "fix": "prettier -w \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "config": {
    "bundle_version": "3.1.55.2-34-ga9934ccd93"
  },
